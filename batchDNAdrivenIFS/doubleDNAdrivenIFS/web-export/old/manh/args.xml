<args>
    <InputFiles> 
        <item>input/HIVmarkov.txt</item>
        <item>input/MethanocalMarkov.txt</item>
    </InputFiles>

	<OutputFile>output/linear/1.txt </OutputFile>
	<GenerationNumber> 100</GenerationNumber>
	<PopulationSize> 100 </PopulationSize>
	<CrossoverFactor> 0.3 </CrossoverFactor>
	<MutationFactor> 0.2 </MutationFactor>
    <FitnessFunction> manhattan </FitnessFunction> 		<!-- possible: manhattan, euclidean, minkowski, linear, mean -->
    <euclFactor> 0.4 </euclFactor>
    <manhFactor> 0.6 </manhFactor>
    <Selection>  tournament </Selection>           <!-- possible: elimination, tournament -->
    <Elitism> 1 </Elitism>                          <!-- possible: 0,1 -->	
	<p>3</p>
	<Batch>30</Batch>
</args>

