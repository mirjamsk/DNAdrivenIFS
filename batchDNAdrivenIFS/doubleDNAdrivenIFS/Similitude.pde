class Similitude {
  float rotate;
  float translateX;
  float translateY;
  float scale;
  color colour;

  Similitude(float rotate, float translateX, float translateY, float scale, color colour) {
    this.rotate = rotate;
    this.translateX = translateX;
    this.translateY = translateY;
    this.scale = scale;
    this.colour = colour;
  }
}

